{
  "_comment": "This is an example configuration showing how to map custom extension attributes between B2C and External ID",
  "Migration": {
    "Export": {
      "SelectFields": "id,userPrincipalName,displayName,givenName,surname,mail,mobilePhone,identities,extension_abc123_LegacyCustomerId,extension_abc123_Department,extension_abc123_EmployeeType"
    },
    "Import": {
      "AttributeMappings": {
        "extension_abc123_LegacyCustomerId": "extension_xyz789_CustomerId",
        "extension_abc123_Department": "extension_xyz789_DepartmentCode",
        "extension_abc123_EmployeeType": "extension_xyz789_UserType"
      },
      "ExcludeFields": [
        "createdDateTime",
        "lastPasswordChangeDateTime"
      ],
      "MigrationAttributes": {
        "StoreB2CObjectId": true,
        "B2CObjectIdTarget": "extension_xyz789_OriginalB2CObjectId",
        "SetRequireMigration": true,
        "RequireMigrationTarget": "extension_xyz789_RequireMigration"
      }
    }
  }
}
